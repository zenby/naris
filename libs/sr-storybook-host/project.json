{
  "name": "sr-storybook-host",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/sr-storybook-host/src",
  "prefix": "soer",
  "implicitDependencies": [
    "sr-editor-blocks",
    "sr-editor",
    "soer-components",
    "naris"
  ],
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/sr-storybook-host/**/*.ts",
          "libs/sr-storybook-host/**/*.html"
        ]
      }
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "libs/sr-storybook-host/.storybook",
        "browserTarget": "sr-storybook-host:build-storybook",
        "styles": [
          "./node_modules/ng-zorro-antd/ng-zorro-antd.min.css",
          "apps/naris/src/styles.scss"
        ],
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputDir": "dist/storybook/sr-storybook-host",
        "configDir": "libs/sr-storybook-host/.storybook",
        "browserTarget": "sr-storybook-host:build-storybook",
        "styles": [
          "./node_modules/ng-zorro-antd/ng-zorro-antd.min.css",
          "apps/naris/src/styles.scss"
        ],
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  },
  "tags": []
}
